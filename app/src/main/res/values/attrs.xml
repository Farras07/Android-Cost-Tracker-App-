<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="custom_card_component">
        <!-- Cards Atrrs-->
        <attr name="cardTitle" format="string" />
        <attr name="cardContent" format="string" />
        <attr name="cardContentTextSize" format="dimension" />
        <attr name="cardIcon" format="reference" />
        <!-- Header Atrrs-->
        <attr name="username" format="string" />
        <attr name="saving" format="integer" />
        <!-- Card Cashflow-->
        <attr name="cardCashflowIcon" format="reference" />
        <attr name="cardCashflowIconCC" format="string" />
        <attr name="cardCashflowDesc" format="string" />
        <attr name="cashflowMoney" format="integer" />
        <attr name="CardCashflowTitle" format="string" />
    </declare-styleable>
</resources>
